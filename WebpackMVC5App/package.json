{
  "name": "WebpackMVC5App",
  "version": "v1.0.0",
  "private": true,
  "description": "Webpack with MVC5",
  "scripts": {
    "start": "npm run webpack:dev:watch",
    "poststart": "npm-run-all test:watch",
    "nsp": "nsp check",
    "clean-build": "npm run remove-build && mkdir build",
    "remove-build": "rimraf ./build",
    "prebuild": "npm-run-all clean-build",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "webpack:dev": "cross-env NODE_ENV=dev webpack --config webpack.config.js",
    "webpack:dev:watch": "cross-env NODE_ENV=dev webpack --config webpack.config.js --watch",
    "webpack": "cross-env NODE_ENV=dev webpack --config wwebpack.config.js"
  },
  "author": "philjohnson3@gmail.com",
  "license": "UNLICENSED",
  "dependencies": {
    "@progress/kendo-ui": "^2017.2.621",
    "@types/bootstrap": "^3.3.35",
    "@types/jquery": "2.0.46",
    "@types/kendo-ui": "^2017.1.7",
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "awesome-typescript-loader": "^3.2.1",
    "babel-core": "^6.25.0",
    "css-loader": "^0.28.4",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "merge-stream": "^1.0.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "less-loader": "^4.0.5",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.2.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-visualizer-plugin": "^0.1.11"
  }
}
